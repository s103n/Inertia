<Window x:Class="InertiaByZeinekken.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InertiaByZeinekken"
        mc:Ignorable="d"
        Title="Menu" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Height="30" Content="Levels"  Click="Button_Click" Grid.Row="0" Grid.Column="1" />
        <Button Height="30" Content="Results" Click="Button_Click1" Grid.Row="1" Grid.Column="1"/>
        <Button Height="30" Content="Exit"  Click="Button_Click3" Grid.Row="3" Grid.Column="1"/>
    </Grid>
</Window>
