<Window x:Class="InertiaByZeinekken.Levels"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InertiaByZeinekken"
        mc:Ignorable="d"
        Title="Levels" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Height="30" Width="70"  Content="Level 1"  Grid.Row="0" Grid.Column="1" Click="Button_Click"/>
        <Button Height="30" Width="70"  Content="Level 2"  Grid.Row="1" Grid.Column="1" Click="Button_Click_1"/>
        <Button Height="30" Width="70"  Content="Level 3"  Grid.Row="2" Grid.Column="1" Click="Button_Click_2"/>
        <Button Height="30" Width="70"  Content="Exit to menu"  Grid.Row="3" Grid.Column="1" Click="Button_Click_3"/>
    </Grid>
</Window>
